@model UserCategoryListModel

<div class="section">
    <h3 class="mt-3 section-header text-orange">Manage Users</h3>
    <hr class="section-divider" />

    <div class="row mt-3">
        <div class="col-md-12">
            <div class="container-box">
                <div class="container-content">
                    <div class="row bg-smoke  p-2" style="width:100%">
                        <h5 class="text-black-50"><i class="fas fa-user-gear mr-2 ml-2"></i>Users</h5>
                    </div>
                    <table class="table table-orchid table-striped table-hover">
                        <thead>
                            <tr>
                                <th>First Name</th>
                                <th>Last Name</th>
                                <th>Email</th>
                            </tr>
                        </thead>
                        <tbody>
                            @foreach (var user in Model.Users)
                            {
                                <tr>
                                    <td>@user.FirstName</td>
                                    <td>@user.LastName</td>
                                    <td>@user.Email</td>
                                </tr>
                            }
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>


